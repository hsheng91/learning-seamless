<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.android.project.TeacherHome">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="My Particulars"
        android:id="@+id/textView77"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:text="View Profiles"
        android:id="@+id/button35"
        android:layout_below="@+id/textView77"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="17dp"
        android:layout_alignRight="@+id/textView77"
        android:layout_alignEnd="@+id/textView77" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="View taught lesson"
        android:id="@+id/textView78"
        android:layout_alignTop="@+id/textView77"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Create Account"
        android:id="@+id/textView79"
        android:layout_centerVertical="true"
        android:layout_alignRight="@+id/button36"
        android:layout_alignEnd="@+id/button36" />

    <Button
        android:layout_width="150sp"
        android:layout_height="wrap_content"
        android:text="Create Student Account"
        android:id="@+id/button36"
        android:layout_above="@+id/button38"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="34dp" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="150sp"
        android:layout_height="wrap_content"
        android:text="Create Teacher Account"
        android:id="@+id/button38"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="View all grade of Students"
        android:id="@+id/textView80"
        android:layout_alignBottom="@+id/textView79"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/textView77"
        android:layout_alignEnd="@+id/textView77" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="View Student Grades"
        android:id="@+id/button37"
        android:layout_alignTop="@+id/button36"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/textView80"
        android:layout_alignEnd="@+id/textView80" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Create Quiz"
        android:id="@+id/button33"
        android:layout_marginBottom="24dp"
        android:layout_above="@+id/textView80"
        android:layout_alignLeft="@+id/textView79"
        android:layout_alignStart="@+id/textView79" />
</RelativeLayout>
